{
  "name": "scd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "tailwindcss": "^1.0.4",
    "typescript": "3.2.1"
  },
  "scripts": {
    "build:tailwind": "postcss src/styles/tailwind.css -o src/index.css",
    "watch:tailwind": "postcss src/styles/tailwind.css -o src/index.css -w",
    "start": "yarn watch:tailwind & react-scripts start",
    "build": "yarn build:tailwind && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^4.1.18",
    "autoprefixer": "^9.6.1",
    "postcss-cli": "^6.1.3",
    "react-icons": "^3.7.0",
    "styled-components": "^4.3.2"
  }
}
